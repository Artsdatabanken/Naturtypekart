{
    "env": {
        "browser": true,
		"jquery": true,
        "es6": false
    },
	"extends": "airbnb/legacy",
	"rules": {
		"indent": 0,
		"quotes": 0,
		"vars-on-top": 0,
		"func-names": 0,
		"max-len": 0,
		"quote-props": 0,
		"new-cap": 0,
		"one-var": 0,
		"spaced-comment": 0,
		"no-trailing-spaces": 0,
		"no-param-reassign": 0,
		"space-before-function-paren": 0,
		"no-unused-vars": 0,
		"no-console": 0,
		"padded-blocks": 0,
		"brace-style": 0,
		"one-var-declaration-per-line": 0,
		"no-script-url": 0,
		"default-case": 0,
		"no-undef": 0,
		"no-useless-escape": 0,
		"no-shadow": 0,
		"keyword-spacing": 0,
		"no-nested-ternary": 0,
		"block-scoped-var": 0,
		"space-infix-ops": 0,
		"dot-notation": 0,
		"no-unused-expressions": 0,
		"no-alert": 0,
		"no-cond-assign": 0,
		"guard-for-in": 0,
		"no-extend-native": 0,
		"consistent-return": 0
	}
}

